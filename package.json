{
  "name": "unit-basic",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "jest": "^28.1.2"
  },
  "dependencies": {
    "@types/jest": "^27.5.2"
  },
  "directories": {
    "test": "test"
  },
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "typeRoots": [
      "./node-modules/@types/",
      "./src/@types/"
    ],
    "types": [
      "jest",
      "node",
      "@types/jest"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ],
  "description": ""
}
